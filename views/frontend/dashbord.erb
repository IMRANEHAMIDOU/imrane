    <style type="text/tailwindcss">
        @layer components{
            .all-customers{
                @apply bg-white rounded-[30px] p-8
            }
            .all-customers .top {
                @apply flex justify-between
            }
            .table{
                @apply min-w-full bg-white border border-gray-300 rounded-lg shadow-sm border-0 mt-8
            }
            .th{
                @apply px-4 py-2 font-semibold text-[#B5B7C0]
            }
            .td{
                @apply px-4 py-2 text-[#292D32]
            }
            .border-t{
                @apply  border-[#EEEEEE] h-[50px]
            }
            .btn{
                @apply px-4 py-2 text-black rounded bg-[#F5F5F5]
            }
            .btn.active{
                @apply bg-[#5932EA] text-white
            }
            .statut{
                @apply px-4 py-1 text-[14px] border-1 rounded block w-[90px] text-center
            }
            .statut.active{
                @apply bg-[#16C09861] border-[#00B087]  text-[#00B087]
            }
            .statut.inactive{
                @apply bg-[#FFC5C5] border-[##DF0404] text-[#DF0404]
            }
            .head{
                @apply grid
            }
        }
    </style>
</head>

